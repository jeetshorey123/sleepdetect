{
  "builds": [
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/streamlit_app.py"
    }
  ],
  "functions": {
    "streamlit_app.py": {
      "includeFiles": ["template.html", "logo.jpg"]
    }
  },
  "env": {
    "PYTHONPATH": "."
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/streamlit_app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/streamlit_app.py"
    }
  ]
}
